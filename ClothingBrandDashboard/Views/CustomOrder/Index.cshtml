@{
    ViewBag.bigtitle = "CustomOrder Management";
    ViewBag.smalltitle = "All Orders ";
    Layout = "~/Views/Shared/_Dashboard.cshtml";
}
@section Styles {
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">

    <style>
        .dt-search {
            display: none;
        }

        .dt-length {
            display: none;
        }
    </style>
}

<div class="container">
    <div class="row align-items-center">
        <div class="col-auto">
            <div class="search-wrapper d-flex align-items-center">
                <label for="startDate" class="me-2">Start Date:</label>
                <input type="date" id="startDate" placeholder="Start Date">
                <label for="endDate" class="me-2 ms-3">End Date:</label>
                <input type="date" id="endDate" placeholder="End Date">
            </div>
        </div>
        <div class="col-auto ms-auto">
            <div class="search-wrapper d-flex align-items-center">
                <i class="fas fa-search" id="searchIcon"></i>
                <input type="text" id="customSearch" placeholder="Search...">
                <a href="/Product/Create" class="btn btn-primary ms-2">Create</a>
            </div>
        </div>
    </div>

</div>


<table id="myTable" class="table table-hover">
    <thead>
        <tr>
            <th>Name</th>
            <th>Design Description</th>
            <th>Order Status</th>
            <th>Date</th>
            <th>Phone</th>
            <th>Action</th>
        </tr>
    </thead>


</table>
@section Scripts {

    @* <h2>hi</h2> *@
    <script src="~/js/CustomOrder.js"></script>



}